"ツールバーなし
set guioptions-=T
"メニューバーなし
set guioptions-=m
"右スクロールバーなし
set guioptions-=r
set guioptions-=R
"左スクロールバーなし
set guioptions-=l
set guioptions-=L
"下スクロールバーなし
set guioptions-=b
"" フォント設定
set guifont=Osaka-mono:h14


" 幅
set columns=130

" 高さ
set lines=35

" コマンドラインの高さ(GUI使用時)
set cmdheight=3

" タブ化
set stal=2

" いつタブページのラベルを表示するかを指定する。
" 0: 表示しない " 1: 2個以上のタブページがあるときのみ表示
" 2: 常に表示
set showtabline=2

colorscheme hybrid

